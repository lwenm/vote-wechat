
.containerview {
    display: flex;
    flex-direction: column;
    height: 100%;
    font-size: 28rpx;
    padding-bottom: 63px;
    margin-top: 14rpx;
    font-family: 'FZPinShangHeiS-R-GB';
}

.itemback {
    display: inherit;
    flex-direction: inherit;
    padding-bottom: 20rpx;
    width: 696rpx;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    margin-left: 29rpx;
    border-radius: 10rpx;
    overflow: visible;
    box-shadow: 2px 1px 8px rgba(0,0,0,0.1);
    position: relative;

}

.item-image {
    width: 100%;
    height: 278rpx;
}

.view-typeA {
    width: 138rpx;
    height: 44rpx;
    margin-left: 27rpx;
    margin-top: -261rpx;
    font-size: 24rpx;
    color: white;
    line-height: 44rpx;
    text-align: center;
    border-radius: 22rpx;
    background: rgb(247,83,96);
}
.view-typeC {
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    width: 138rpx;
    height: 44rpx;
    font-size: 24rpx;
    color: white;
    line-height: 44rpx;
    text-align: center;
    /* border-radius: 22rpx; */
    /* border-top-left-radius: 22rpx; */
    border-bottom-left-radius: 22rpx;
    background: rgb(247,83,96);
}
.view-typeB {
    width: 138rpx;
    height: 44rpx;
    margin-left: 27rpx;
    margin-top: -261rpx;
    font-size: 24rpx;
    color: white;
    line-height: 44rpx;
    text-align: center;
    border-radius: 22rpx;
    background: rgba(0,0,0,0.3);
}

.back-title {
    display: inherit;
    margin-left: 28rpx;
}

.back-titleA {
    margin-top: 241rpx;
}

.back-titleB {
    margin-top: 24rpx;
}

.back-gift {
    display: flex;
    flex-direction: row;
    margin-top: 7rpx;
}

.back-cup {
    display: inherit;
    flex-direction: column;
    justify-content: flex-start;
}

.image-cup {
    margin-left: 24rpx;
    width: 24rpx;
    height: 24rpx;
    margin-top: 8rpx;
}

.text-gift {
    width: 325rpx;
    margin-left: 14rpx;
}

.back-bonus {
    display: flex;
    flex-direction: row;
    margin-top: 7rpx;
    align-items: center;
}

.image-cash {
    margin-left: 24rpx;
    width: 24rpx;
    height: 24rpx;
}

.text-bonus {
    width: 325rpx;
    margin-left: 14rpx;
}

.back-endtime {
    display: inherit;
    flex-direction: column;
    justify-content: flex-end;
}

.text-endtime {
    font-size: 22rpx;
    color: rgba(0,0,0,0.5);
    margin-right: 30rpx;
    text-align: right;
    width: 275rpx;
    margin-top: 6rpx;
}

.text-endcounttime {
    font-size: 22rpx;
    color: rgba(0,0,0,0.5);
    text-align: left;
    margin-left: 28rpx;
}

.view-loadMore {
    font-size: 28rpx;
    color: #3399FF;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.back-loadMore {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.back-novote {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 750rpx;
    padding-bottom: 150rpx;
}

.image-novote-head {
    width: 416rpx;
    height: 418rpx;
    margin-top: 90rpx;
}

.text-novote {
    font-size: 28rpx;
    margin-top: 68rpx;
    color: rgba(0,0,0,0.4);
    text-align: center;
    line-height: 33rpx;
}

.text-novoteA {
    margin-top: 13rpx;
}

.image-novote-down {
    width: 24rpx;
    height: 144rpx;
    margin-top: 136rpx;
}

.view-submit {
    font-weight: bold;
    font-size: 30rpx;
    text-align: center;
    height: 96rpx;
    border-radius: 48rpx;
    line-height: 96rpx;
    margin-top: 70rpx;
    color: white;
    background: linear-gradient(to right,rgb(245,43,85),rgb(255,218,39));
    margin-left: 36rpx;
    width: 678rpx;
}
